<script>
	function readDataGrid(Json){
		var txtsend = new Array();
		for (keys in Json)
		{
			if (Json[keys].data)
			{										
				var myJSONText = JSON.stringify(Json[keys].data, function (key, value) {return value;	});										
				txtsend.push(myJSONText);
			}
		}								
		return	'['+txtsend.join(',')+']';
	}
</script>
